    "#ID_settings\n",
    "# import required libraries and settings\n",
    "import pandas as pd\n",
    "import glob\n",
    "import numpy as np\n",
    "import csv\n",
    "import openai\n",
    "import random\n",
    "import re\n",
    "import time \n",
    "import os\n",
    "import matplotlib.pyplot as plt\n",
    "from tqdm import tqdm\n",
    "import ipywidgets as widgets\n",
    "from IPython.display import display\n",
    "\n",
    "\n",
    "# Create the 'TVDL' dropdown menu with options\n",
    "TT = widgets.Dropdown(\n",
    "    options=['1', '3','5'],\n",
    "    value='1',\n",
    "    description='Test Time'\n",
    ")\n",
    "\n",
    "def list_of_files():\n",
    "    # List all files in the prompt folder\n",
    "    files = glob.glob(\"Prompts/*.txt\")\n",
    "    files = [f.split(\"/\")[-1] for f in files]\n",
    "    files = [f.split(\".\")[0] for f in files]\n",
    "    #files = np.array(files)\n",
    "    return files    \n",
    "\n",
    "print(list_of_files())\n",
    "\n",
    "\n",
    "\n",
    "# Create the 'prompt' dropdown menu with options\n",
    "# prompt_dropdown = widgets.Dropdown(\n",
    "\n",
    "#     options=['None 0', 'None 1', 'None 2', 'Generic 0','Generic 1','Generic 2', 'Specific 0', 'Specific 1', 'Specific 2'],\n",
    "#     value='None 0',\n",
    "#     description='Prompt:'\n",
    "# )\n",
    "\n",
    "def create_prompt_dropdown(folder_path=\"../Prompts\"):\n",
    "\n",
    "    # Step 1: Get the prompt files from the directory\n",
    "    current_dir = os.getcwd()\n",
    "    \n",
    "    # Walk up the directory tree to find the Prompts folder\n",
    "    while True:\n",
    "        # Construct the path to the Prompts folder\n",
    "        prompts_path = os.path.join(current_dir, \"Prompts\")\n",
    "\n",
    "        # Check if the path exists\n",
    "        if os.path.isdir(prompts_path):\n",
    "            break  # Found the prompts folder, exit the loop\n",
    "\n",
    "        # Move up one directory\n",
    "        parent_dir = os.path.dirname(current_dir)\n",
    "\n",
    "        # If we reach the root, break\n",
    "        if parent_dir == current_dir:\n",
    "            raise FileNotFoundError(f\"The folder '{folder_name}' could not be found.\")\n",
    "        \n",
    "        current_dir = parent_dir\n",
    "\n",
    "    prompt_files = os.listdir(prompts_path)\n",
    "\n",
    "    # Step 2: Define the regular expression to match filenames and extract relevant parts\n",
    "    pattern = re.compile(r'prompts_ID_([\\w\\s]+)\\s(\\d+)\\.txt')\n",
    "\n",
    "    # Step 3: Iterate over files and extract names and numbers for the dropdown options\n",
    "    options = []\n",
    "    for file in prompt_files:\n",
    "        match = pattern.match(file)\n",
    "        if match:\n",
    "            prompt_name = match.group(1)  # Extract the prompt name\n",
    "            prompt_number = match.group(2)  # Extract the prompt number\n",
    "            options.append(f'{prompt_name} {prompt_number}')\n",
    "\n",
    "    # Step 4: Create the dropdown with the extracted options\n",
    "    prompt_dropdown = widgets.Dropdown(\n",
    "        options=options,\n",
    "        value=options[0] if options else None,  # Default value is the first option if options exist\n",
    "        description='Prompt:'\n",
    "    )\n",
    "    \n",
    "    return prompt_dropdown\n",
    "\n",
    "# Create the 'Model' dropdown menu with options\n",
    "model_dropdown = widgets.Dropdown(\n",
    "    options=['gpt-3.5-turbo', 'gpt-4-1106-preview'],\n",
    "    value='gpt-3.5-turbo',\n",
    "    description='Model:'\n",
    ")\n",
    "\n",
    "# Create NumberBoxes for 'Number of Development' and 'Number of Experiments'\n",
    "num_development = widgets.IntText(\n",
    "    value=10,\n",
    "    description='Dev. Cycle:',\n",
    "    layout={'width': 'auto'}\n",
    ")\n",
    "\n",
    "num_experiments = widgets.IntText(\n",
    "    value=5,\n",
    "    description='Experiments:',\n",
    "    layout={'width': 'auto'}\n",
    ")\n",
    "\n",
    "# Create a TextBox for 'Model-Temperatures'\n",
    "model_temperatures = widgets.Text(\n",
    "    value='0',\n",
    "    description='Temperature Setting:',\n",
    "    layout={'width': 'auto'}\n",
    ")\n",
    "\n",
    "# Create the FloatSlider for 'Target' as a percentage\n",
    "targ_quant = widgets.FloatSlider(\n",
    "    value=99,\n",
    "    min=50,\n",
    "    max=100,\n",
    "    step=1,\n",
    "    description='Target:',\n",
    "    layout={'width': 'auto'}\n",
    ")\n",
    "\n",
    "conversation_dropdown = widgets.Dropdown(\n",
    "    options=['Complete', 'Exceptions+Results'],\n",
    "    value='Exceptions+Results',\n",
    "    description='Display Conversation:'\n",
    ")\n",
    "\n",
    "# Arrange the widgets in two columns\n",
    "left_column = widgets.VBox([model_dropdown, num_development, num_experiments, conversation_dropdown])\n",
    "#right_column = widgets.VBox([prompt_dropdown, model_temperatures,targ_quant, TT])\n",
    "right_column = widgets.VBox([create_prompt_dropdown(), model_temperatures,targ_quant, TT])\n",
    "# Create an overall layout with two columns\n",
    "main_layout = widgets.HBox([left_column, right_column])\n",
    "title = widgets.HTML(\"<h2>Settings of Inverse Design Experiments</h2>\")\n",
    "settings = widgets.VBox([title, main_layout])\n"
   ]
  }
